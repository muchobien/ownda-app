{
  "name": "ownda",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "patch-package",
    "android": "react-native run-android",
    "android:dev": "yarn android --variant=developmentDebug --appIdSuffix=dev",
    "android:preview": "yarn android --variant=previewRelease --appIdSuffix=preview",
    "ios": "react-native run-ios",
    "ios:dev": "yarn ios --scheme ownda-dev",
    "ios:preview": "yarn ios --scheme ownda-preview --configuration Release",
    "start": "react-native start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "gradlew": "scripts/gradlew",
    "pod": "scripts/pod",
    "typecheck": "scripts/typecheck",
    "eas-build-pre-install": "bash -c \"[ ! -z \\\"$EAS_BUILD_NPM_CACHE_URL\\\" ] && sed -i -e \\\"s#https://registry.yarnpkg.com#$EAS_BUILD_NPM_CACHE_URL#g\\\" yarn.lock\" || true",
    "eas-build-post-install": "scripts/eas-build-post-install"
  },
  "expo": {
    "autolinking": {
      "exclude": [
        "expo-keep-awake",
        "react-native-reanimated"
      ]
    }
  },
  "dependencies": {
    "@emotion/native": "11.0.0",
    "@emotion/react": "11.8.2",
    "@expo-google-fonts/inter": "0.2.2",
    "@nozbe/watermelondb": "0.24.0",
    "@react-navigation/bottom-tabs": "6.2.0",
    "@react-navigation/devtools": "6.0.5",
    "@react-navigation/native": "6.0.8",
    "@react-navigation/native-stack": "6.5.1",
    "dayjs": "1.11.0",
    "expo": "44.0.6",
    "expo-application": "4.0.2",
    "expo-blur": "11.0.0",
    "expo-file-system": "13.2.1",
    "expo-font": "10.0.5",
    "expo-linear-gradient": "11.0.3",
    "expo-local-authentication": "12.1.1",
    "ramda": "0.28.0",
    "react": "17.0.2",
    "react-hook-form": "7.28.1",
    "react-native": "0.67.4",
    "react-native-bootsplash": "4.1.3",
    "react-native-dotenv": "3.3.1",
    "react-native-gesture-handler": "2.3.2",
    "react-native-mmkv": "2.2.0",
    "react-native-mmkv-flipper-plugin": "1.0.0",
    "react-native-pager-view": "6.0.0-rc.1",
    "react-native-reanimated": "2.5.0",
    "react-native-safe-area-context": "4.2.2",
    "react-native-screens": "3.13.1",
    "react-native-svg": "12.3.0"
  },
  "devDependencies": {
    "@arkweid/lefthook": "0.7.7",
    "@babel/core": "7.17.8",
    "@babel/plugin-proposal-decorators": "7.17.8",
    "@babel/plugin-transform-react-jsx": "7.17.3",
    "@babel/runtime": "7.17.8",
    "@muchobien/eslint-config-react-native": "0.0.8",
    "@types/ramda": "0.28.1",
    "@types/react-native": "0.67.3",
    "babel-plugin-module-resolver": "4.1.0",
    "eslint": "8.11.0",
    "metro-react-native-babel-preset": "0.69.1",
    "patch-package": "6.4.7",
    "postinstall": "0.7.4",
    "postinstall-postinstall": "2.1.0",
    "prettier": "2.6.0",
    "react-native-flipper": "0.138.0",
    "typescript": "4.6.2"
  },
  "resolutions": {
    "@types/react": "^17"
  }
}
