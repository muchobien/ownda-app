{
  "name": "ownda",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:dev": "yarn android --variant=developmentDebug --appIdSuffix=dev",
    "android:preview": "yarn android --variant=previewRelease --appIdSuffix=preview",
    "ios": "react-native run-ios",
    "ios:dev": "yarn ios --scheme ownda-dev",
    "ios:preview": "yarn ios --scheme ownda-preview --configuration Release",
    "start": "react-native start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "codegen": "graphql-codegen",
    "gradlew": "scripts/gradlew",
    "pod": "scripts/pod",
    "typecheck": "scripts/typecheck",
    "eas-build-pre-install": "bash -c \"[ ! -z \\\"$EAS_BUILD_NPM_CACHE_URL\\\" ] && sed -i -e \\\"s#https://registry.yarnpkg.com#$EAS_BUILD_NPM_CACHE_URL#g\\\" yarn.lock\" || true",
    "eas-build-post-install": "scripts/eas-build-post-install"
  },
  "expo": {
    "autolinking": {
      "exclude": [
        "expo-keep-awake",
        "expo-application"
      ]
    }
  },
  "dependencies": {
    "@apollo/client": "3.5.9",
    "@emotion/native": "11.0.0",
    "@emotion/react": "11.8.1",
    "@expo-google-fonts/inter": "0.2.2",
    "@react-navigation/bottom-tabs": "6.2.0",
    "@react-navigation/devtools": "6.0.5",
    "@react-navigation/native": "6.0.8",
    "@react-navigation/native-stack": "6.5.0",
    "apollo-link-token-refresh": "0.4.0",
    "apollo3-cache-persist": "0.14.0",
    "dayjs": "1.10.8",
    "expo": "44.0.6",
    "expo-blur": "11.0.0",
    "expo-file-system": "13.2.1",
    "expo-font": "10.0.5",
    "expo-linear-gradient": "11.0.3",
    "expo-local-authentication": "12.1.1",
    "graphql": "16.3.0",
    "jwt-decode": "3.1.2",
    "ramda": "0.28.0",
    "react": "17.0.2",
    "react-hook-form": "7.27.1",
    "react-native": "0.67.3",
    "react-native-bootsplash": "4.1.3",
    "react-native-dotenv": "3.3.1",
    "react-native-gesture-handler": "2.3.0",
    "react-native-mmkv": "2.1.0",
    "react-native-mmkv-flipper-plugin": "1.0.0",
    "react-native-reanimated": "2.4.1",
    "react-native-safe-area-context": "3.4.1",
    "react-native-screens": "3.13.0",
    "react-native-svg": "12.2.0"
  },
  "devDependencies": {
    "@arkweid/lefthook": "0.7.7",
    "@babel/core": "7.17.5",
    "@babel/plugin-transform-react-jsx": "7.17.3",
    "@babel/runtime": "7.17.2",
    "@graphql-codegen/add": "3.1.1",
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/schema-ast": "2.4.1",
    "@graphql-codegen/typed-document-node": "2.2.5",
    "@graphql-codegen/typescript": "2.4.5",
    "@graphql-codegen/typescript-apollo-client-helpers": "2.1.12",
    "@graphql-codegen/typescript-operations": "2.3.2",
    "@muchobien/eslint-config-react-native": "0.0.5",
    "@types/ramda": "0.28.0",
    "@types/react-native": "0.67.1",
    "babel-plugin-module-resolver": "4.1.0",
    "eslint": "8.10.0",
    "metro-react-native-babel-preset": "0.69.0",
    "postinstall": "0.7.4",
    "prettier": "2.5.1",
    "react-native-flipper": "0.136.0",
    "typescript": "4.6.2"
  },
  "resolutions": {
    "@types/react": "^17"
  }
}
