{
  "name": "ownda",
  "version": "0.0.3",
  "private": true,
  "scripts": {
    "postversion": "react-native-version --generate-build",
    "postinstall": "patch-package",
    "android": "react-native run-android",
    "android:dev": "yarn android --variant=developmentDebug --appIdSuffix=dev",
    "android:preview": "yarn android --variant=previewRelease --appIdSuffix=preview",
    "ios": "react-native run-ios",
    "ios:dev": "yarn ios --scheme ownda-dev",
    "ios:preview": "yarn ios --scheme ownda-preview --configuration Release",
    "start": "react-native start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "gradlew": "scripts/gradlew",
    "pod": "scripts/pod",
    "typecheck": "scripts/typecheck",
    "eas-build-pre-install": "bash -c \"[ ! -z \\\"$EAS_BUILD_NPM_CACHE_URL\\\" ] && sed -i -e \\\"s#https://registry.yarnpkg.com#$EAS_BUILD_NPM_CACHE_URL#g\\\" yarn.lock\" || true"
  },
  "expo": {
    "autolinking": {
      "exclude": [
        "expo-keep-awake",
        "react-native-reanimated"
      ]
    }
  },
  "dependencies": {
    "@emotion/native": "11.0.0",
    "@emotion/react": "11.9.3",
    "@expo-google-fonts/inter": "0.2.2",
    "@gorhom/bottom-sheet": "4.3.2",
    "@nozbe/watermelondb": "0.24.0",
    "@react-native-community/hooks": "2.8.1",
    "@react-navigation/bottom-tabs": "6.3.1",
    "@react-navigation/devtools": "6.0.7",
    "@react-navigation/native": "6.0.10",
    "@react-navigation/native-stack": "6.6.2",
    "@shopify/react-native-skia": "0.1.130",
    "dayjs": "1.11.3",
    "expo": "45.0.5",
    "expo-application": "4.1.0",
    "expo-blur": "11.1.0",
    "expo-file-system": "14.0.0",
    "expo-font": "10.1.0",
    "expo-linear-gradient": "11.3.0",
    "expo-local-authentication": "12.2.0",
    "expo-localization": "13.0.0",
    "ramda": "0.28.0",
    "react": "17.0.2",
    "react-hook-form": "7.32.0",
    "react-native": "0.68.2",
    "react-native-bootsplash": "4.1.5",
    "react-native-dotenv": "3.3.1",
    "react-native-fast-image": "8.5.11",
    "react-native-flipper-databases": "2.4.1",
    "react-native-gesture-handler": "2.4.2",
    "react-native-graph": "0.1.1",
    "react-native-mmkv": "2.4.1",
    "react-native-mmkv-flipper-plugin": "1.0.0",
    "react-native-pager-view": "6.0.0-rc.2",
    "react-native-reanimated": "2.8.0",
    "react-native-safe-area-context": "4.3.1",
    "react-native-screens": "3.13.1",
    "react-native-svg": "12.3.0"
  },
  "devDependencies": {
    "@arkweid/lefthook": "0.7.7",
    "@babel/core": "7.18.5",
    "@babel/plugin-proposal-decorators": "7.18.2",
    "@babel/plugin-transform-react-jsx": "7.17.12",
    "@babel/runtime": "7.18.3",
    "@muchobien/eslint-config-react-native": "0.1.4",
    "@muchobien/react-native-version": "git+https://github.com/muchobien/react-native-version.git",
    "@types/ramda": "0.28.14",
    "@types/react-native": "0.67.8",
    "babel-plugin-module-resolver": "4.1.0",
    "eslint": "8.17.0",
    "metro-react-native-babel-preset": "0.71.1",
    "patch-package": "6.4.7",
    "postinstall-postinstall": "2.1.0",
    "prettier": "2.6.2",
    "react-native-flipper": "0.146.1",
    "typescript": "4.7.3"
  },
  "resolutions": {
    "@types/react": "^17"
  }
}
